{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "./popup/popup.html",
      "default_icon": "./images/light-bulb.png"
    },
    "permissions": [
      "tabs",
      "storage",
      "offscreen"
    ],
    "background": {
        "service_worker": "background.js"
      }
  }